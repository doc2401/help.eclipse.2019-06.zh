<?xml version="1.0" encoding="UTF-8"?>
<?NLS TYPE="org.eclipse.help.contexts"?>
<contexts>
    <!-- Actions -->
    <context id="goto_referenced_test_action_context">
        <description>此命令导航到引用此Java元素的测试。</description>
    </context>
    <context id="enable_filter_action_context">
        <description>切换JUnit首选项页面中定义的堆栈跟踪条目的过滤。</description>
        <topic label="JUnit首选项" href="reference/preferences/java/ref-preferences-junit.htm"/>
    </context>
    <context id="junit_tab_group">
        <description>此启动配置运行JUnit测试。</description>
        <topic label="使用JUnit" href="gettingStarted/qs-junit.htm"/>
    </context>
    <context id="launch_shortcut">
        <description>此命令启动JUnit测试。选择类时，将运行其测试。选择项目，包或源文件夹后，将运行所有包含的测试。选择单个测试方法后，将运行所选测试。
</description>
         <topic label="使用JUnit" href="gettingStarted/qs-junit.htm"/>
    </context>
    <context id="copy_trace_action_context">
        <description>此命令将堆栈跟踪复制到剪贴板。</description>
    </context>
    <context id="open_test_action_context">
        <description>此命令将打开相应的测试用例。</description>
    </context>
    <context id="open_editor_atline_action_context">
        <description>此命令将打开相应的测试方法。</description>
    </context>
    <context id="rerun_test_action_context">
        <description>重新选择所选测试。</description>
    </context>
    <context id="scroll_lock">
        <description>锁定测试结果视图，以便不会自动显示当前执行的测试。</description>
    </context>
    
    <!-- View parts -->
    <context id="results_view_context">
        <description>该视图显示测试结果。</description>
        <topic label="使用JUnit" href="gettingStarted/qs-junit.htm"/>
    </context>
    
    <!-- Wizard pages -->
    <context id="new_testcase_wizard_page_context">
        <description>该向导可帮助您创建新的测试用例并为现有方法生成测试存根。</description>
        <topic label="使用JUnit" href="gettingStarted/qs-junit.htm"/>
    </context>
    <context id="new_testcase_wizard_page2_context">
        <description>该向导可帮助您创建新的测试用例并为现有方法生成测试存根。</description>
        <topic label="使用JUnit" href="gettingStarted/qs-junit.htm"/>
    </context>
    <context id="new_testsuite_wizard_page2_context">
        <description>该向导可帮助您创建新的测试套件。</description>
        <topic label="使用JUnit" href="gettingStarted/qs-junit.htm"/>
    </context>
    <context  id="launch_configuration_dialog_junit_main_tab" >
        <description>此选项卡允许您指定要执行的JUnit测试。</description>
        <topic label="使用JUnit" href="gettingStarted/qs-junit.htm"/>
    </context>
   
    <!-- Dialogs -->
    <context id="test_selection_context">
        <description>用于选择测试的对话框。</description>
   </context>
    <context id="result_compare_context">
        <description>此对话框将预期与文本比较视图中的实际测试结果进行比较。</description>
    </context>
    
    <!-- Preference pages -->
    <context id="junit_preference_page_context">
        <description>您可以在此页面中设置JUnit首选项。</description>
        <topic label="使用JUnit" href="gettingStarted/qs-junit.htm"/>
        <topic label="JUnit首选项" href="reference/preferences/java/ref-preferences-junit.htm"/>
    </context>
</contexts>
