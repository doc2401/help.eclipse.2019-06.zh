<?xml version="1.0" encoding="UTF-8"?>
<?NLS TYPE="org.eclipse.help.contexts"?>
<contexts>

	<!-- ********************************** -->
	<!-- Dialogs							-->
	<!-- ********************************** -->
	
	<context id="sync_view_preference_dialog_context">
		<description>此对话框允许您在“同步视图”中设置配置活动同步。</description>
	</context>

	<context id="configre_refresh_schedule_dialog_context">
		<description>此对话框允许您将活动同步配置为以指定间隔自动刷新。</description>
	</context>

	<!-- ********************************** -->
	<!-- Wizard Pages						-->
	<!-- ********************************** -->

	<context id="share_project_page_context">
		<description>此页面可帮助您选择要将项目连接到的目标站点类型。</description>
	</context>

	<context id="import_project_set_page_context">
		<description>此页面可帮助您从文件系统上的文件或直接从URL导入项目集。项目集是一组共享项目的轻量级描述。它可用于重建工作区中的共享项目阵容。</description>
	</context>

	<context id="export_project_set_page_context">
		<description>此页面可帮助您将项目集导出到可与其他团队成员共享的文件。项目集是一组共享项目的轻量级描述。它可用于重建工作区中的共享项目阵容。</description>
	</context>

	<context id="sync_resource_selection_page_context">
		<description>此页面允许您选择要同步的资源。</description>
	</context>

	<context id="refresh_wizard_selection_page_context">
		<description>此页面允许您选择要创建的同步类型。</description>
	</context>

	<!-- ********************************** -->
	<!-- Preference Pages					-->
	<!-- ********************************** -->

	<context id="ignore_preference_page_context">
		<description>您可能希望从版本控制中排除某些资源。名称与任何已启用模式匹配的资源将不会被释放。通配符'*'和'？'是允许的。示例：'/ * / a / *'匹配'/ x / a /'中的所有文件和子文件夹以及'/ x / y / z / a /'，'/？/ a / *'匹配所有文件和子文件夹在'/ x / a /'，'/？/ a / * .doc'匹配'/ x / a /'文件夹及其子文件夹中的所有.doc文件。这些设置不会影响已在版本控制下的资源。</description>
		<topic label="创建全局忽略模式" href="tasks/tasks-100d1.htm"/>
	</context>

	<context id="model_preference_page_context">
		<description>此页面允许您在“同步”视图中管理可用的模型。</description>
		<topic label="CVS同步视图" href="reference/ref-33.htm"/>
	</context>

	<context id="sync_preference_page_context">
	<description>此页面允许您配置Synchronize View的各个方面。</description>
		<topic label="CVS同步视图" href="reference/ref-33.htm"/>
	</context>

	<context id="sync_startup_preference_page_context">
		<description>此页面允许您配置Eclipse启动时是否自动填充Synchronize视图。</description>
	</context>

	<context id="resource_model_preference_page_context">
		<description>此页面允许您配置文件系统资源在同步视图中的显示方式。</description>
	</context>

	<context id="file_type_preference_page_context">
		<description>此页面允许您将文件扩展名与其内容类型（ASCII或二进制）相关联。这些设置由存储库提供程序使用，例如CVS</description>
		<topic label="设置文件扩展名的内容类型" href="tasks/tasks-team-1.htm"/>
	</context>

	<!-- ********************************** -->
	<!-- TargetMenuActions 					-->
	<!-- ********************************** -->
	
	<context id="target_get_action_context">
		<description>将所选资源从站点下载到工作区。</description>
	</context>

	<context id="target_put_action_context">
		<description>将所选资源从工作区上载到站点。</description>
	</context>

	<context id="target_synchronize_action_context">
		<description>在“同步”视图中显示所选资源的本地和远程站点状态之间的任何差异。</description>
	</context>

	<context id="target_getas_action_context">
		<description>将所选资源从站点下载到工作区。您可以指定资源的本地名称。</description>
	</context>

	<context id="target_getasproject_action_context">
		<description>将所选资源从站点下载到工作区。在工作区中为每个选定项目创建项目。</description>
	</context>

	<context id="target_disconnect_action_context">
		<description>从记住的站点列表中丢弃选定的目标站点。</description>
	</context>

	<!-- ********************************** -->
	<!-- TargetCatchupReleaseViewer			-->
	<!-- ********************************** -->

	<context id="target_catchup_release_viewer_context">
		<description>此视图显示传入，传出和冲突的更改。</description>
	</context>

	<!-- ********************************** -->
	<!-- TargetActions						-->
	<!-- ********************************** -->

	<context id="sync_get_action_context">
		<description>将所选资源从站点下载到工作区。</description>
	</context>

	<context id="sync_put_action_context">
		<description>将所选资源从工作区上载到站点。</description>
	</context>

	<context id="share_project_action_context">
		<description>使用Eclipse中提供的任何版本控制管理系统共享所选项目。</description>
	</context>

	<context id="apply_patch_action_context">
		<description>将修补程序应用于工作区中的文件和文件夹。</description>
		<topic label="使用补丁" href="tasks/tasks-68c.htm"/>
	</context>

	<!-- ********************************** -->
	<!-- Views								-->
	<!-- ********************************** -->

	<context id="site_explorer_view_context">
		<description>站点资源管理器视图允许您浏览已知目标站点的内容并将远程资源下载到本地工作区。</description>
	</context>

	<context id="sync_view_context">
		<description>“同步”视图允许将工作空间的本地内容与远程存储库或目标同步。</description>
		<topic label="CVS同步视图" href="reference/ref-33.htm"/>
	</context>

	<context id="local_history_context">
		<description>“本地历史记录”视图允许用户查看文件的本地历史记录或文件中包含的元素。</description>
		<topic label="本地历史" href="gettingStarted/qs-55.htm"/>
		<topic label="本地历史偏好" href="reference/ref-14.htm"/>
		<topic label="使用本地历史记录" href="tasks/tasks-1f.htm"/>
	</context>

	<!-- ********************************** -->
	<!-- SiteExplorerViewActions			-->
	<!-- ********************************** -->

	<context id="add_site_action_context">
		<description>创建可以上载或下载资源的新目标站点。</description>
	</context>

	<context id="new_folder_action_context">
		<description>在目标站点上创建一个新文件夹。</description>
	</context>

	<!-- ********************************** -->
	<!-- SyncViewActions					-->
	<!-- ********************************** -->

	<context id="refresh_action_context">
		<description>刷新视图的内容。</description>
	</context>

	<context id="open_action_context">
		<description>在比较查看器中显示文件的本地和远程内容的比较。</description>
	</context>

	<context id="expandall_action_context">
		<description>完全展开所有选定的项目。</description>
	</context>

	<context  id="remove_action_context">
		<description>从“同步”视图中删除所选项目。</description>
	</context>

	<context  id="navigator_show_action_context">
		<description>在“导航器”视图中选择并显示所选项目。</description>
	</context>

</contexts>