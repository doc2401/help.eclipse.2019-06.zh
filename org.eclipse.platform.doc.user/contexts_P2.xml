<?xml version="1.0" encoding="UTF-8"?>
<?NLS TYPE="org.eclipse.help.contexts"?>
<contexts>
    <!-- Dialogs -->
    
    <context id="installed_software_context">
        <description>在此页面中，您可以使用您的安装。</description>
        <topic label="更新安装" href="tasks/tasks-120.htm"/>   
        <topic label="卸载软件" href="tasks/tasks-126.htm"/>
     </context>
    
     <context id="revert_configuration_wizard_context">
        <description>在此页面中，您可以查看安装历史记录并将安装恢复到以前的状态。</description>
        <topic label="恢复到以前的安装配置" href="tasks/tasks-123.htm"/>   
    </context>

    <context id="uinstall_wizard_context">
        <description>在此向导中，您可以从安装中卸载项目。</description>
       <topic label="卸载软件" href="tasks/tasks-126.htm"/>
    </context>
 
    <context id="update_wizard_context">
        <description>在此向导中，您可以更新所选项目。</description>
       <topic label="使用“可用更新”向导更新软件" href="tasks/tasks-125.htm"/>
    </context>

    <context id="add_repository_dialog_context">
        <description>在此对话框中，您可以添加新软件站点以安装或更新该站点中的项目。</description>
       <topic label="添加新的更新站点" href="tasks/tasks-127.htm"/>
    </context>

    <context id="repository_manipulation_dialog_context">
        <description>此页面允许您添加和删除软件站点，或选择在安装新软件或更新安装时应参考的站点。</description>
       <topic label="使用可用软件站点" href="tasks/tasks-128.htm"/>
    </context>

    <context id="install_wizard_context">
        <description>在此向导中，您可以从一个或多个软件站点安装新软件。</description>
         <topic label="安装新软件" href="tasks/tasks-124.htm"/>
     </context>

    <!-- Preference Pages -->
    <context id="provisioning_preference_page_context">
        <description>此页面允许您配置常规安装/更新首选项。</description>
       <topic label="首选项 - 安装/更新" href="reference/ref-p2-installupdate.htm"/>
    </context>

    <context id="automatic_updates_preference_page_context">
        <description>此页面允许您配置自动更新的首选项。</description>
       <topic label="首选项 - 自动更新" href="reference/ref-p2-autoupdate.htm"/>
    </context>
</contexts>
