<?xml version="1.0" encoding="ISO-8859-1"?>
<?NLS TYPE="org.eclipse.help.contexts"?>

<contexts>
	<context id="coverage_view_context">
		<description>此视图允许沿Java元素层次结构向下钻取coverage数据。</description>
		<topic label="使用覆盖范围视图" href="pages/coverageview.html"/>
		<topic label="管理覆盖范围会话" href="pages/sessions.html"/>
	</context>
	<context id="coverage_properties_context">
		<description>显示所选Java元素的所有coverage计数器。</description>
		<topic label="覆盖范围属性" href="pages/coverageproperties.html"/>
	</context>
	<context id="coverage_preferences_context">
		<description>调整代码覆盖功能的行为。</description>
		<topic label="代码覆盖率首选项" href="pages/preferences.html"/>
	</context>
	<context id="coverage_launch_context">
		<description>在覆盖模式下启动此应用程序类型。</description>
		<topic label="以覆盖模式启动" href="pages/launching.html"/>
	</context>
	<context id="coverage_launch_tab_context">
		<description>此选项卡指定覆盖率分析的范围。</description>
		<topic label="以覆盖模式启动" href="pages/launching.html"/>
	</context>
	<context id="dump_execution_data">
		<description>从正在运行的coverage启动转储执行数据。</description>
	<topic label="以覆盖模式启动" href="pages/launching.html"/>
	<topic label="管理覆盖范围会话" href="pages/sessions.html"/>
	</context>
	<context id="select_active_session_context">
		<description>选择应显示的覆盖会话。</description>
		<topic label="管理覆盖范围会话" href="pages/sessions.html"/>
	</context>
	<context id="merge_sessions_context">
		<description>将选定的覆盖会话合并为单个会话。</description>
		<topic label="管理覆盖范围会话" href="pages/sessions.html"/>
	</context>
	<context id="session_export_context">
		<description>以不同格式导出代码覆盖会话。</description>
		<topic label="会话导入和导出" href="pages/importexport.html"/>
	</context>
	<context id="session_import_context">
		<description>导入外部执行的代码覆盖会话，以便在IDE中进行分析。</description>
		<topic label="会话导入和导出" href="pages/importexport.html"/>
	</context>
</contexts>
