<contexts>

<!-- Actions -->
	<context id="open_manifest">
		<description>此操作将打开所选插件项目中包含的清单文件。</description>
	</context>
	<context id="organize_manifest">
		<description>此操作可组织和清除所选插件项目中包含的清单文件。</description>
		<topic label="组织清单文件" href="guide/tools/pde_actions/organize_manifests.htm"/>
	</context>
	<context id="update_classpath">
		<description>PDE通过查找工作空间中的依赖项并启用外部插件和片段来计算此项目的类路径。</description>
		<topic label="更新类路径" href="guide/tools/pde_actions/update_classpaths.htm"/>
	</context>
	<context id="open_dependencies">
		<description>此命令将打开“依赖关系”视图，以显示运行所选插件所需的所有插件。</description>
		<topic label="依赖页面" href="guide/tools/editors/manifest_editor/dependencies.htm"/>
	</context>	
	<context id="find_references">
		<description>此命令搜索直接依赖于所选插件的所有插件以及以其为目标的所有片段。</description>
